{
  "kind": "collectionType",
  "collectionName": "tmbaccommodations",
  "info": {
    "singularName": "tmbaccommodation",
    "pluralName": "tmbaccommodations",
    "displayName": "tmbaccommodation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Refuge",
        "Hotel",
        "B&B",
        "Campsite"
      ]
    },
    "location_type": {
      "type": "enumeration",
      "enum": [
        "On-trail",
        "Near-trail",
        "Off-trail"
      ]
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    },
    "website": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "price_range": {
      "type": "string"
    },
    "capacity": {
      "type": "integer"
    },
    "altitude": {
      "type": "integer"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "notes": {
      "type": "text"
    },
    "booking_method": {
      "type": "enumeration",
      "enum": [
        "Online Portal",
        "Email",
        "Phone",
        "Multiple"
      ]
    },
    "stage": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tmb-stage.tmb-stage"
    },
    "Accommodation_Service": {
      "type": "component",
      "component": "accommodation.service",
      "repeatable": true
    },
    "description": {
      "type": "text"
    },
    "booking_difficulty": {
      "type": "enumeration",
      "enum": [
        "Easy",
        "Moderate",
        "Hard",
        "Very Hard"
      ]
    }
  }
}
